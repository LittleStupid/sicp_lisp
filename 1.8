(define (cube x) (* x x x))

(define (good-enough? guess x)
  (< (abs
        (- (cube guess)
            x))
      0.001)
)

(define (improve guess x)
  (/ (+ (/ x (* guess guess))
        (* 2 guess))
     3)
)

(define (cubrt-iter x guess)
  (if (good-enough? guess x)
      guess
      (cubrt-iter x
                  (improve guess x))))

(define (cubrt x))
  (cubrt-iter x 1.0)

(cubrt 27)
(cubrt 1000)
(cubrt 1)
(cubrt 100)
